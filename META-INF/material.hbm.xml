<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>    
    <class name="org.eve.mm.material.Material" table="MAT001">
        <id name="id" column="IDENT"/>
        <property name="reference" column="REFER"/>
        <property name="netWeight" column="PSLIQ"/>
        <property name="grossWeight" column="PSBRT"/>
        <property name="weightUnit" column="UNDPS"/>
        <property name="quantity" column="QUANT"/>
        <property name="quantityUnit" column="UNDQT"/>
        <property name="netPrice" column="VLLIQ"/>
        <property name="priceUnit" column="UNVLQ"/>
        <property name="regDate" column="DTREG"/>
        <property name="regTime" column="TMREG"/>
        <property name="regUser" column="USREG"/>
    </class>
</hibernate-mapping>
